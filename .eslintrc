rules:
  indent:
    - 0
    - tab
  quotes:
    - 0
    - single
  linebreak-style:
    - 2
    - unix
  semi:
    - 2
    - never
  no-console:
    - 0
  no-unused-vars:
    - 0
  comma-dangle:
    - 0
env:
  es6: true
  node: true
  browser: true
extends: 'eslint:recommended'
ecmaFeatures:
  jsx: true
  experimentalObjectRestSpread: true
  modules: true
plugins:
  - react
